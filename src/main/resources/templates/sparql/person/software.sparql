PREFIX vivo: <http://vivoweb.org/ontology/core#>
PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

CONSTRUCT {
    ?software rdfs:label ?labelWithId .
} WHERE {
    <{{uri}}> vivo:relatedBy ?authorship .
    ?authorship a vivo:Authorship .
    ?authorship vivo:relates ?software .
    ?software a obo:ERO_0000071 .
    ?software rdfs:label ?label .
    BIND( CONCAT( STR(?label), REPLACE(STR(?software), "(^.*/)", "::") ) AS ?labelWithId ) .
}